      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [
        ""
      ],
      "stopAtEntry": false,
      "cwd": "/home/bug/Documentos/Projectos .Ignite/RENTALX/src/modules/cars",
      "environment": [],
      "program": "/home/bug/Documentos/Projectos .Ignite/RENTALX/src/modules/cars/build/Debug/outDebug",
      "internalConsoleOptions": "openOnSessionStart",
      "MIMode": "gdb",
